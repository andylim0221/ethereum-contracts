{
  "manifestVersion": "2.2",
  "contracts": {},
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.0.0"
  },
  "name": "@paypr/ethereum-contracts",
  "version": "0.0.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": true,
        "runs": "200"
      }
    },
    "typechain": {
      "enabled": true,
      "outDir": "./types/contracts/",
      "target": "web3-v1"
    },
    "manager": "openzeppelin",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts",
    "solcVersion": "0.8.3"
  },
  "telemetryOptIn": false
}
